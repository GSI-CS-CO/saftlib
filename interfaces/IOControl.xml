<node xmlns:xi="http://www.w3.org/2001/XInclude" name="IOControl">
  <annotation name="de.gsi.saftlib.name" value="de.gsi.saftlib"/>
  
  <!-- Include base interfaces -->
  <xi:include href="Owned.xml"/>
  
  <!-- de.gsi.saftlib.IOControl:
      @short_description: IOControl is used to configure IO direction, resistors and more.
   -->
  <interface name="de.gsi.saftlib.IOControl">
    
    <!-- SetParameter: Pass a parameter to the module. -->
    <method name="SetParameter">
      <arg direction="in" type="u" name="param"/>
      <arg direction="in" type="u" name="value"/>
    </method>
    
    <!-- GetParameter: Get a parameter from the module. -->
    <method name="GetParameter">
      <arg direction="in"  type="u" name="param"/>
      <arg direction="out" type="u" name="value"/>
    </method>
    
    <!-- CheckIoName: Check if given IO name really exists. -->
    <method name="CheckIoName">
      <arg direction="in"  type="s" name="name"/>
      <arg direction="out" type="u" name="value"/>
    </method>
    
    <!-- GetIoTableParameterByNumber: Get parameters from the self describing IO table by number. -->
    <method name="GetIoTableParameterByNumber">
      <arg direction="in"  type="u" name="number"/>
      <arg direction="in"  type="u" name="param"/>
      <arg direction="out" type="u" name="value"/>
    </method>
    
    <!-- GetIoTableParameterByName: Get parameters from the self describing IO table by name. -->
    <method name="GetIoTableParameterByName">
      <arg direction="in"  type="s" name="name"/>
      <arg direction="in"  type="u" name="param"/>
      <arg direction="out" type="u" name="value"/>
    </method>
    
    <!-- GetIoTableName: Get IO name by number/index. -->
    <method name="GetIoTableName">
      <arg direction="in"  type="u" name="number"/>
      <arg direction="out" type="s" name="value"/>
    </method>
    
    <!-- GetIoTableNumber: Get table number/index ID by name. -->
    <method name="GetIoTableNumber">
      <arg direction="in"  type="s" name="name"/>
      <arg direction="out" type="u" name="number"/>
    </method>
    
    <!-- GetInternalId: Get internal ID by name. -->
    <method name="GetInternalId">
      <arg direction="in"  type="s" name="name"/>
      <arg direction="out" type="u" name="number"/>
    </method>
    
    <!-- GetDirection: Get direction by name. -->
    <method name="GetDirection">
      <arg direction="in"  type="s" name="name"/>
      <arg direction="out" type="u" name="number"/>
    </method>
    
    <!-- GetChannel: Get channel by name. -->
    <method name="GetChannel">
      <arg direction="in"  type="s" name="name"/>
      <arg direction="out" type="u" name="number"/>
    </method>
    
    <method name="SetOe">
      <arg direction="in"  type="s" name="name"/>
      <arg direction="out" type="u" name="status"/>
    </method>
    
    <method name="ResetOe">
      <arg direction="in"  type="s" name="name"/>
      <arg direction="out" type="u" name="status"/>
    </method>
    
    <method name="GetOe">
      <arg direction="in"  type="s" name="name"/>
      <arg direction="out" type="u" name="status"/>
    </method>
    
    <method name="SetTerm">
      <arg direction="in"  type="s" name="name"/>
      <arg direction="out" type="u" name="status"/>
    </method>
    
    <method name="ResetTerm">
      <arg direction="in"  type="s" name="name"/>
      <arg direction="out" type="u" name="status"/>
    </method>
    
    <method name="GetTerm">
      <arg direction="in"  type="s" name="name"/>
      <arg direction="out" type="u" name="status"/>
    </method>
    
    <method name="SetSpecIn">
      <arg direction="in"  type="s" name="name"/>
      <arg direction="out" type="u" name="status"/>
    </method>
    
    <method name="ResetSpecIn">
      <arg direction="in"  type="s" name="name"/>
      <arg direction="out" type="u" name="status"/>
    </method>
    
    <method name="GetSpecIn">
      <arg direction="in"  type="s" name="name"/>
      <arg direction="out" type="u" name="status"/>
    </method>
    
    <method name="SetSpecOut">
      <arg direction="in"  type="s" name="name"/>
      <arg direction="out" type="u" name="status"/>
    </method>
    
    <method name="ResetSpecOut">
      <arg direction="in"  type="s" name="name"/>
      <arg direction="out" type="u" name="status"/>
    </method>
    
    <method name="GetSpecOut">
      <arg direction="in"  type="s" name="name"/>
      <arg direction="out" type="u" name="status"/>
    </method>
    
    <method name="SetMux">
      <arg direction="in"  type="s" name="name"/>
      <arg direction="out" type="u" name="status"/>
    </method>
    
    <method name="ResetMux">
      <arg direction="in"  type="s" name="name"/>
      <arg direction="out" type="u" name="status"/>
    </method>
    
    <method name="GetMux">
      <arg direction="in"  type="s" name="name"/>
      <arg direction="out" type="u" name="status"/>
    </method>
    
    <method name="DriveLow">
      <arg direction="in"  type="s" name="name"/>
      <arg direction="out" type="u" name="status"/>
    </method>
    
    <method name="DriveHigh">
      <arg direction="in"  type="s" name="name"/>
      <arg direction="out" type="u" name="status"/>
    </method>
    
    <method name="GetOutput">
      <arg direction="in"  type="s" name="name"/>
      <arg direction="out" type="u" name="status"/>
    </method>
    
    <method name="GetOutputCombined">
      <arg direction="in"  type="s" name="name"/>
      <arg direction="out" type="u" name="status"/>
    </method>
    
    <method name="GetInput">
      <arg direction="in"  type="s" name="name"/>
      <arg direction="out" type="u" name="status"/>
    </method>
    
    <!-- GPIOInputs: Number of GPIO inputs. -->
    <property name="GPIOInputs"    type="u" access="read"/>
    
    <!-- GPIOInoutputs: Number of GPIO inouts. -->
    <property name="GPIOInoutputs" type="u" access="read"/>
    
    <!-- GPIOOutputs: Number of GPIO outputs. -->
    <property name="GPIOOutputs"   type="u" access="read"/>
    
    <!-- GPIOTotal: Number of all GPIOs (in/inout/out). -->
    <property name="GPIOTotal"     type="u" access="read"/>
    
    <!-- LVDSInputs: Number of LVDS inputs. -->
    <property name="LVDSInputs"    type="u" access="read"/>
    
    <!-- LVDSInoutputs: Number of LVDS inouts. -->
    <property name="LVDSInoutputs" type="u" access="read"/>
    
    <!-- LVDSOutputs: Number of LVDS outputs. -->
    <property name="LVDSOutputs"   type="u" access="read"/>
    
    <!-- LVDSTotal: Number of all LVDS (in/inout/out) IOs. -->
    <property name="LVDSTotal"     type="u" access="read"/>
    
    <!-- FIXEDTotal: Number of fixed (uncontrollable) IOs. -->
    <property name="FIXEDTotal"    type="u" access="read"/>
    
    <!-- IOVersion: Version of the IO control module. -->
    <property name="IOVersion"     type="u" access="read"/>
    
    <!-- DeviceName: Device name -->
    <property name="DeviceName"    type="s" access="read"/>
    
  </interface>
</node>
